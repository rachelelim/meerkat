#include "meerkatTestApp.h"
#include "meerkatApp.h"
#include "Moose.h"
#include "AppFactory.h"
#include "MooseSyntax.h"

InputParameters
meerkatTestApp::validParams()
{
  InputParameters params = meerkatApp::validParams();
  return params;
}

meerkatTestApp::meerkatTestApp(InputParameters parameters) : MooseApp(parameters)
{
  meerkatTestApp::registerAll(
      _factory, _action_factory, _syntax, getParam<bool>("allow_test_objects"));
}

meerkatTestApp::~meerkatTestApp() {}

void
meerkatTestApp::registerAll(Factory & f, ActionFactory & af, Syntax & s, bool use_test_objs)
{
  meerkatApp::registerAll(f, af, s);
  if (use_test_objs)
  {
    Registry::registerObjectsTo(f, {"meerkatTestApp"});
    Registry::registerActionsTo(af, {"meerkatTestApp"});
  }
}

void
meerkatTestApp::registerApps()
{
  registerApp(meerkatApp);
  registerApp(meerkatTestApp);
}

/***************************************************************************************************
 *********************** Dynamic Library Entry Points - DO NOT MODIFY ******************************
 **************************************************************************************************/
// External entry point for dynamic application loading
extern "C" void
meerkatTestApp__registerAll(Factory & f, ActionFactory & af, Syntax & s)
{
  meerkatTestApp::registerAll(f, af, s);
}
extern "C" void
meerkatTestApp__registerApps()
{
  meerkatTestApp::registerApps();
}
